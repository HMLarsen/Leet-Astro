---
import Dashboard from "../components/dashboard/Dashboard.svelte";
import BaseLayout from "./BaseLayout.astro";

interface Props {
	title: string;
}
const { title } = Astro.props;
---

<BaseLayout title={"Dashboard - " + title} description="Gerencie seus eventos.">
	<Dashboard client:only="svelte" title={title}>
		<slot />
	</Dashboard>
</BaseLayout>
